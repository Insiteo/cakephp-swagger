{
    "name": "alt3/cakephp-swagger",
    "description": "Swagger plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "zircote/swagger-php": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Alt3\\Swagger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alt3\\Swagger\\Test\\": "tests"
        }
    }
}
