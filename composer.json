{
    "name": "alt3/cakephp-swagger",
    "description": "Instant Swagger documentation for your CakePHP 3.x APIs",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "zircote/swagger-php": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Alt3\\Swagger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alt3\\Swagger\\Test\\": "tests"
        }
    }
}
